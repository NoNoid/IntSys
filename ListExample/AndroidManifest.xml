<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shuetzenApp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.listviewtest.SessionListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.listviewtest.SeriesListActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.example.listviewtest.SessionListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.listviewtest.SessionListActivity" />
        </activity>
        <activity
            android:name="com.example.listviewtest.Details"
            android:label="@string/title_activity_details"
            android:parentActivityName="com.example.listviewtest.SeriesListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.listviewtest.SeriesListActivity" />
        </activity>
        <activity
            android:name="com.example.shuetzenApp.NewSession"
            android:label="@string/title_activity_new_session" >
        </activity>
    </application>

</manifest>
